{
  "name": "etsy.js",
  "version": "0.0.1",
  "description": "Latest Etsy API wrapper for NodeJS.",
  "private": false,
  "main": "./src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "jest --detectOpenHandles --testPathPattern=test/",
    "test:dev": "jest --watchAll --testPathPattern=test/",
    "format": "prettier --write . && eslint src --fix",
    "lint": "prettier --check . && eslint src",
    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml",
    "publish": "<publis npm>"
  },
  "keywords": [
    "etsy",
    "etsy.js",
    "etsy-api",
    "etsy-api-wrapper",
    "etsy-api-wrapper-node",
    "api",
    "api-wrapper",
    "api-wrapper-node",
    "wrapper",
    "wrapper-node"
  ],
  "dependencies": {
    "axios": "^1.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-angular": "^17.4.4",
    "@playerberry/eslint-config": "^1.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yankikucuk/etsy.js.git"
  },
  "bugs": {
    "url": "https://github.com/yankikucuk/etsy.js/issues"
  },
  "author": "Yankı Küçük <yankikucuk@playerberry.com> (https://playerberry.com)",
  "contributors": [
    "ex.codes <kendinikertenkelebek@me.com>"
  ],
  "license": "Apache-2.0"
}
